{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ProductName": {
      "type": "string"
    },
    "ProductDesc": {
      "type": "text"
    },
    "ProductImg": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "ProductPrice": {
      "type": "integer"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "ProductStatus": {
      "type": "boolean"
    },
    "ProductVisibility": {
      "type": "boolean"
    },
    "ProductStock": {
      "type": "boolean"
    },
    "ProductSale": {
      "type": "boolean"
    },
    "ProductQuantity": {
      "type": "biginteger"
    }
  }
}
