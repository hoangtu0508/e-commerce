{
  "kind": "collectionType",
  "collectionName": "carts",
  "info": {
    "singularName": "cart",
    "pluralName": "carts",
    "displayName": "Cart"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ProductName": {
      "type": "string"
    },
    "ProductDesc": {
      "type": "text"
    },
    "ProductPrice": {
      "type": "biginteger"
    },
    "ProductImg": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "inversedBy": "cart"
    },
    "qty": {
      "type": "biginteger"
    }
  }
}
